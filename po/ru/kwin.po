#, kde-format
msgid "ICC profile \"%1\" is not usable for displays"
msgstr "ICC-профиль «%1» нельзя использовать для экранов"

#: core/iccprofile.cpp:343
#, kde-format
msgid ""
"ICC profile \"%1\" has unsupported connection space, only XYZ is supported"

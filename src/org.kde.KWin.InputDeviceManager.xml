<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.KWin.InputDeviceManager">
    <property name="devicesSysNames" type="as" access="read"/>
    <method name="ListPointers">
      <arg type="as" direction="out"/>
    </method>
    <method name="ListKeyboards">
      <arg type="as" direction="out"/>
    </method>
    <method name="ListTouch">
      <arg type="as" direction="out"/>
    </method>
    <signal name="deviceAdded">
      <arg name="sysName" type="s" direction="out"/>
    </signal>
    <signal name="deviceRemoved">
      <arg name="sysName" type="s" direction="out"/>
    </signal>
  </interface>
</node>
